<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:id="@+id/slidepanelchildtwo_topviewtwo"
	android:layout_width="match_parent"
	android:orientation="vertical"
	android:background="@drawable/min_rounded_dialog"
	android:layout_height="match_parent">
	<androidx.cardview.widget.CardView
		android:id="@+id/cardview2"
		android:layout_gravity="fill"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		app:cardBackgroundColor="@color/gnt_black"
		app:cardCornerRadius="10dp"
		android:layout_margin="5dp"
		app:cardElevation="2dp">
		<FrameLayout
			android:id="@+id/Mainroot"
			android:clickable="true"
			android:layout_width="match_parent"
			android:layout_height="210dp">
			<FrameLayout
				android:id="@+id/root"
				android:minWidth="210dp"
				android:layout_gravity="fill"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:minHeight="99dp">
				<YouTubePlayerAndroidX.Player.YouTubePlayerView
					android:id="@+id/youtube_player_view"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:paddingEnd="5dp"
					android:minHeight="210dp"/>
				<com.google.android.exoplayer2.ui.PlayerView
					android:id="@+id/player_view"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:minHeight="210dp"
					android:paddingEnd="5dp"
					app:resize_mode="fit" />
				<ProgressBar
					android:id="@+id/progress_bar"
					android:layout_width="45dp"
					android:layout_height="45dp"
					android:layout_gravity="center"
					android:visibility="invisible"/>
			</FrameLayout>
		</FrameLayout>
	</androidx.cardview.widget.CardView>
	<androidx.coordinatorlayout.widget.CoordinatorLayout
		android:id="@+id/parent"
		android:translationY="0dp"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="?attr/colorFill"
		android:orientation="vertical">
		<com.google.android.material.appbar.AppBarLayout
			android:id="@+id/appBarLayout"
			android:layout_width="match_parent"
			android:layout_marginTop="0dp"
			app:elevation="0dp"
			android:layout_marginBottom="0dp"
			android:background="?attr/colorFill"
			android:layout_height="wrap_content">
			<com.google.android.material.appbar.CollapsingToolbarLayout
				android:id="@+id/htab_collapse_toolbar"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				app:scrimVisibleHeightTrigger="10dp"
				app:contentScrim="?attr/colorFill"
				app:layout_scrollFlags="scroll|exitUntilCollapsed"
				app:titleEnabled="false">
				<LinearLayout
					android:paddingBottom="15dp"
					android:layout_marginBottom="0dp"
					android:background="?attr/colorFill"
					android:orientation="vertical"
					android:layout_width="match_parent"
					android:layout_height="wrap_content">

					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal"
						android:weightSum="10"
						android:padding="8dp"
						android:layout_gravity="center_vertical"
						android:gravity="center_vertical">

						<TextView
							android:id="@+id/video_Titile"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:textSize="15dp"
							android:layout_weight="10"
							android:typeface="normal"
							android:text="Top 10 Unsportsmanlike Moments in Pro Top 10 Unsportsmanlike Moments in Pro Sports"
							android:gravity="start"
							android:maxLines="2" />

						<View
							android:layout_width="0dp"
							android:layout_height="1dp"
							android:layout_weight="1" />

						<TextView
							android:id="@+id/video_ShowDiscription"
							android:layout_width="30dp"
							android:layout_height="wrap_content"
							android:lines="1"
							android:gravity="center"
							android:layout_gravity="top|end"
							android:background="?android:attr/selectableItemBackgroundBorderless"
							android:textColor="?attr/colorReact"
							android:textSize="20dp"
							android:text=">"/>
					</LinearLayout>

					<TextView
						android:id="@+id/info"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:textSize="13dp"
						android:layout_marginTop="8dp"
						android:layout_marginStart="8dp"
						android:textColor="?attr/colorReact"
						android:text="2M Views | 10K Shares | 500 Comments | 3 Months Ago"
						android:gravity="start" />

					<LinearLayout
						android:layout_marginTop="15dp"
						android:layout_width="match_parent"
						android:layout_height="50dp"
						android:orientation="horizontal"
						android:minWidth="100px"
						android:id="@+id/ButtonLayout"
						android:minHeight="50px"
						android:layout_marginStart="5dp"
						android:layout_marginEnd="5dp">
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:id="@+id/LikeButton"
							android:layout_gravity="center_vertical"
							android:gravity="center_vertical">
							<ImageView
								android:id="@+id/Likeicon"
								android:layout_width="24dp"
								android:layout_height="24dp"
								android:src="@drawable/icon_likefilled_video_vector"
								android:tint="#a7a7a7"
								android:layout_gravity="center"
								android:gravity="center" />
							<TextView
								android:id="@+id/LikeNumber"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="12sp"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="0"
								android:layout_marginTop="5dp" />
						</LinearLayout>
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_gravity="center_vertical"
							android:gravity="center_vertical"
							android:id="@+id/UnLikeButton">
							<ImageView
								android:id="@+id/UnLikeicon"
								android:layout_width="24dp"
								android:layout_height="24dp"
								android:src="@drawable/icon_likefilled_video_vector"
								android:tint="#a7a7a7"
								android:rotation="180"
								android:layout_gravity="center"
								android:gravity="center" />
							<TextView
								android:id="@+id/UnLikeNumber"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="12sp"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="0"
								android:layout_marginTop="5dp" />
						</LinearLayout>
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_gravity="center_vertical"
							android:gravity="center_vertical"
							android:id="@+id/editButton">
							<TextView
								android:id="@+id/editIcon"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="21sp"
								android:textStyle="bold"
								android:includeFontPadding="false"
								android:textColor="#a7a7a7"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="Ic" />
							<TextView
								android:id="@+id/editNumber"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="12sp"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/Lbl_Edit"
								android:layout_marginTop="5dp" />
						</LinearLayout>
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_gravity="center_vertical"
							android:gravity="center_vertical"
							android:id="@+id/DonateButton">
							<ImageView
								android:id="@+id/donateicon"
								android:layout_width="24dp"
								android:layout_height="24dp"
								android:src="@drawable/icon_video_donate_vector"
								android:tint="#a7a7a7"
								android:layout_gravity="center"
								android:gravity="center" />
							<TextView
								android:id="@+id/donatetext"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="12sp"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/Lbl_Donate"
								android:layout_marginTop="5dp" />
						</LinearLayout>
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_gravity="center_vertical"
							android:gravity="center_vertical"
							android:id="@+id/RentButton">
							<ImageView
								android:id="@+id/renticon"
								android:layout_width="24dp"
								android:layout_height="24dp"
								android:src="@drawable/ic_video_rent_vector"
								android:tint="#a7a7a7"
								android:layout_gravity="center"
								android:gravity="center" />
							<TextView
								android:id="@+id/renttext"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="12sp"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/Lbl_Rent"
								android:layout_marginTop="5dp" />
						</LinearLayout>
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_gravity="center_vertical"
							android:gravity="center_vertical"
							android:id="@+id/ShareButton">
							<TextView
								android:id="@+id/Shareicon"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="21sp"
								android:textStyle="bold"
								android:includeFontPadding="false"
								android:textColor="#a7a7a7"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="Ic" />
							<TextView
								android:id="@+id/ShareText"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="12sp"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/Lbl_Share"
								android:layout_marginTop="5dp" />
						</LinearLayout>
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="0.5"
							android:layout_gravity="center_vertical"
							android:gravity="center_vertical"
							android:id="@+id/AddToButton">
							<TextView
								android:id="@+id/AddToicon"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="21sp"
								android:textStyle="bold"
								android:includeFontPadding="false"
								android:textColor="#a7a7a7"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="Ic" />
							<TextView
								android:id="@+id/AddToNumber"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textSize="12sp"
								android:layout_gravity="center"
								android:gravity="center"
								android:text="@string/Lbl_Add_To"
								android:layout_marginTop="5dp" />
						</LinearLayout>
					</LinearLayout>

					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="vertical"
						android:background="?attr/colorSecondaryExtra"
						android:padding="15dp">

						<LinearLayout
							android:id="@+id/ChannelLayout"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/Image_Channel"
								android:layout_width="40dp"
								android:layout_height="40dp"
								android:scaleType="centerCrop"
								android:layout_gravity="start|center_vertical"
								android:gravity="start|center_vertical"
								android:src="@drawable/no_profile_image_circle"
								android:layout_marginStart="8dp"
								android:layout_marginEnd="8dp" />
							<FrameLayout
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:orientation="horizontal">
								<LinearLayout
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_gravity="center_vertical"
									android:gravity="center_vertical"
									android:orientation="vertical">
									<TextView
										android:id="@+id/ChannelName"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:drawablePadding="5dp"
										android:textSize="15sp"
										android:text="Wael anjo"
										android:textStyle="bold" />
									<TextView
										android:id="@+id/Channelviews"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:text="11.2K Subscribers"
										android:layout_marginTop="0dp"
										android:textSize="13sp"
										android:textColor="?attr/colorReact" />
								</LinearLayout>
								<androidx.appcompat.widget.AppCompatButton
									android:layout_width="120dp"
									android:layout_height="40dp"
									android:id="@+id/SubcribeButton"
									android:textSize="14sp"
									android:layout_gravity="end|center"
									android:layout_marginStart="11dp"
									android:layout_marginEnd="10dp"
									android:background="@drawable/round_button_pressed"
									android:text="@string/Lbl_Subscribe"
									android:tag="false"
									android:paddingTop="1dp"
									android:paddingStart="17dp"
									android:paddingEnd="17dp"
									android:textAlignment="center"
									android:textAllCaps="false"
									android:forceDarkAllowed="false"
									android:textColor="@color/gnt_white" />
							</FrameLayout>
						</LinearLayout>


						<androidx.recyclerview.widget.RecyclerView
							android:id="@+id/recyler"
							android:clickable="true"
							android:focusable="false"
							android:focusableInTouchMode="true"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:nestedScrollingEnabled="false"
							android:scrollbars="vertical"
							android:paddingStart="5dp"
							android:paddingEnd="5dp"
							android:layout_marginTop="8dp" />

					</LinearLayout>

					<LinearLayout
						android:id="@+id/videoCategoryLayout"
						android:layout_width="match_parent"
						android:orientation="horizontal"
						android:layout_height="wrap_content"
						android:layout_marginTop="5dp"
						android:layout_marginBottom="0dp">
						<TextView
							android:id="@+id/videoCategoryDefaultText"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:textSize="14sp"
							android:includeFontPadding="false"
							android:layout_gravity="center"
							android:gravity="start"
							android:layout_centerVertical="true"
							android:text="@string/Lbl_Category"
							android:layout_marginStart="10dp"
							android:layout_marginEnd="10dp"
							android:layout_marginTop="2dp"
							android:layout_marginBottom="2dp"
							android:textStyle="bold" />
						<TextView
							android:id="@+id/videoCategorytextview"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:textSize="14sp"
							android:layout_centerVertical="true"
							android:includeFontPadding="false"
							android:textColor="@color/accent"
							android:layout_gravity="center"
							android:gravity="center_vertical"
							android:layout_marginStart="14dp"
							android:layout_marginEnd="10dp"
							android:layout_marginBottom="2dp"
							android:layout_marginTop="2dp" />
					</LinearLayout>

					<LinearLayout
						android:id="@+id/videoDescriptionLayout"
						android:layout_width="match_parent"
						android:orientation="vertical"
						android:layout_height="wrap_content"
						android:layout_marginTop="5dp"
						android:visibility="gone"
						android:tag="closed">
						<TextView
							android:id="@+id/videoDate"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:textSize="13sp"
							android:textStyle="bold"
							android:includeFontPadding="false"
							android:layout_gravity="start"
							android:gravity="start"
							android:text="@string/Lbl_Published_on"
							android:layout_marginStart="10dp"
							android:layout_marginEnd="10dp"
							android:layout_marginTop="1dp" />
						<PlayTube.Library.Anjo.SuperTextLibrary.SuperTextView
							android:id="@+id/videoDescriptionTextview"
							android:layout_gravity="start"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:textSize="13dp"
							android:ellipsize="end"
							android:layout_marginStart="10dp"
							android:layout_marginEnd="10dp"
							android:layout_marginTop="15dp"
							android:layout_marginBottom="10dp" /> 
					</LinearLayout>
				</LinearLayout>
			</com.google.android.material.appbar.CollapsingToolbarLayout>
			<RelativeLayout
				android:layout_marginTop="0dp"
				android:padding="0dp"
				app:layout_collapseMode="pin"
				android:background="?attr/colorFill"
				android:layout_width="match_parent"
				android:layout_height="42dp">
				<ImageView
					android:id="@+id/viewMoreCommentsection"
					android:layout_alignParentStart="true"
					android:rotation="90"
					android:visibility="gone"
					android:translationY="0dp"
					android:layout_centerVertical="true"
					android:layout_marginBottom="0dp"
					android:layout_marginStart="5dp"
					android:src="@drawable/icon_view_more_vector"
					android:layout_width="30dp"
					android:layout_height="30dp" />
				<TextView
					android:layout_toEndOf="@+id/viewMoreCommentsection"
					android:text="@string/Lbl_NextTo"
					android:id="@+id/UpNextTextview"
					android:textStyle="bold"
					android:textSize="15dp"
					android:translationY="0dp"
					android:layout_centerVertical="true"
					android:layout_marginBottom="0dp"
					android:layout_marginTop="0dp"
					android:layout_marginStart="8.5dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
				<Switch
					android:layout_below="@+id/videoDescriptionLayout"
					android:text="@string/Lbl_AutoPlay"
					android:id="@+id/AutoNextswitch"
					android:textStyle="bold"
					android:textSize="12dp"
					android:translationY="0dp"
					android:textAllCaps="true"
					android:layout_centerVertical="true"
					android:textColor="@color/accent"
					android:layout_alignParentEnd="true"
					android:layout_marginTop="0dp"
					android:layout_marginEnd="10dp"
					android:layout_marginBottom="11dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			</RelativeLayout>
		</com.google.android.material.appbar.AppBarLayout>
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			app:layout_behavior="@string/appbar_scrolling_view_behavior">
			<FrameLayout
				android:layout_alignParentTop="true"
				android:id="@+id/videoButtomLayout"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"/>
			<RelativeLayout
				android:clickable="true"
				android:translationY="0dp"
				android:focusable="true"
				android:layout_alignParentBottom="true"
				android:id="@+id/CommentButtomLayout"
				android:background="@drawable/rounded_dialog"
				android:layout_width="match_parent"
				android:layout_gravity="bottom"
				android:gravity="bottom"
				android:layout_height="48dp">
				<ImageView
					android:id="@+id/viewMoreCommentsection"
					android:layout_alignParentStart="true"
					android:rotation="270"
					android:tint="?attr/colorReact"
					android:layout_centerVertical="true"
					android:visibility="visible"
					android:layout_marginBottom="5dp"
					android:layout_marginStart="3dp"
					android:src="@drawable/icon_view_more_vector"
					android:layout_width="30dp"
					android:layout_height="30dp" />
				<TextView
					android:layout_toEndOf="@+id/viewMoreCommentsection"
					android:text="@string/Lbl_Comments"
					android:id="@+id/UpNextTextview"
					android:textStyle="normal"
					android:textSize="15dp"
					android:layout_centerVertical="true"
					android:layout_marginBottom="0dp"
					android:layout_marginTop="0dp"
					android:layout_marginStart="4dp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content" />
			</RelativeLayout>
		</RelativeLayout>
	</androidx.coordinatorlayout.widget.CoordinatorLayout>
</LinearLayout>